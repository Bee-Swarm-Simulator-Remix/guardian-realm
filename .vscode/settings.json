{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true,
        "source.fixAll": "explicit"
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "window.commandCenter": true,
    "workbench.experimental.share.enabled": true,
    "C_Cpp.files.exclude": {
        "extensions/**": true,
        "build/**": true,
        "src/**": true,
        "tmp/**": true,
        "tests/**": true,
        "node_modules/**": true
    },
    "cSpell.words": [
        "automoderation",
        "bitfield",
        "blazebuild",
        "blazew",
        "botinfo",
        "Buildable",
        "Memoizable",
        "oclif",
        "openai",
        "SOFTBAN",
        "softbanned",
        "Stringable",
        "sudobot",
        "Sweeped",
        "tsbuildinfo"
    ],
    "material-icon-theme.folders.associations": {
        "automod": "Server",
        "framework": "Lib",
        "policies": "Controller",
        "permissions": "Resolver",
        "build_src": "Tools",
        "polyfills": "Interface",
        "arguments": "Messages"
    },
    "typescript.preferences.importModuleSpecifier": "relative"
}
